<% layout('layouts/boilerplate')%>
 
   
   

<div class="body_div d-flex justify-content-center">
               
    <% for( let i = orders.length-1; i>=0; i-- ) { %>
     <div class="card">
         <div class="card-header">
            Order:<%= i+1 %>
         </div>
         <div class="card-body">
           
           <a href="/<%= currentuser._id %>/orders/<%= i %>" class="btn btn-info"> expand</a>
         </div>
       </div>

      
    
    <% } %>  

</div>